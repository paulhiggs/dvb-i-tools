			                      ((&Hex16;:){4}(((:&Hex16;){1,3})|((:&Hex16;)?:&IPv4Address;)|:))|
			                      ((&Hex16;:){3}(((:&Hex16;){1,4})|((:&Hex16;){0,2}:&IPv4Address;)|:))|
			                      ((&Hex16;:){2}(((:&Hex16;){1,5})|((:&Hex16;){0,3}:&IPv4Address;)|:))|
			                      ((&Hex16;:){1}(((:&Hex16;){1,6})|((:&Hex16;){0,4}:&IPv4Address;)|:))|
			                      (:(((:&Hex16;){1,7})|((:&Hex16;){0,5}:&IPv4Address;)|:)))(%.+)?\s*$/"/>
