<?xml version="1.0" encoding="UTF-8"?>
<ClassificationScheme uri="urn:tva:metadata:cs:SubtitleCodingFormatCS:2023">
	<!-- ##################################################################### -->
	<!-- SUBTITLECODINGFORMAT                                                  -->
	<!-- DEFINITION: Defines the coding format used for subtitles.             -->
	<!-- NOTE: This CS replaces the CaptionCodingFormatCS.                     -->
	<!-- ##################################################################### -->
	<Term termID="1">
		<Name xml:lang="en">WST Subtitles</Name>
		<Definition xml:lang="en">World System Teletext subtitles.</Definition>
	</Term>
	<Term termID="2">
		<Name xml:lang="en">DVB Subtitles</Name>
		<Definition xml:lang="en">Digital Video Broadcasting Subtitles [ETSI EN 300 743].</Definition>
		<Term termID="2.1">
			<Name>DVB Subtitles (bitmaps)</Name>
			<Definition>DVB subtitling service containing object_data_segment{}s with
                object_coding_method=0x00.</Definition>
			<Term termID="2.1.2">
				<Name>Compliance with version 1.1.1 and 1.2.1 of [ETSI EN 300 743]</Name>
				<Definition>Indicative service compatibility with SDTV.</Definition>
			</Term>
			<Term termID="2.1.3">
				<Name>Compliance with version 1.3.1 of [ETSI EN 300 743]</Name>
				<Definition>Indicative service compatibility with HDTV, SDTV as defined in V1.3.1 of 
                    [ETSI EN 300 743].</Definition>
			</Term>
			<Term termID="2.1.4">
				<Name>Compliance with version 1.4.1 and 1.5.1 of [ETSI EN 300 743]</Name>
				<Definition>Indicative service compatibility with 3DTV, HDTV, SDTV.</Definition>
			</Term>
			<Term termID="2.1.5">
				<Name>Compliance with version V1.6.1 of [ETSI EN 300 743]</Name>
				<Definition>Indicative service compatibility with HDTV, UHDTV, 3DTV, SDTV as defined 
                    in V1.6.1 of [ETSI EN 300 743].</Definition>
			</Term>
		</Term>
		<Term termID="2.2">
			<Name>DVB Subtitles (characters)</Name>
			<Definition>DVB subtitling service containing object_data_segment{}s with object_coding_method=0x01.</Definition>
		</Term> 
	</Term>
